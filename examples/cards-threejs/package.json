{
  "name": "cards",
  "version": "1.0.0",
  "description": "cards example",
  "main": "dist/cards.js",
  "author": "Corban Riley",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html",
    "tslint": "tslint -c tslint.json $(find src -type f -name *.ts)",
    "tslint:fix": "tslint -c tslint.json --fix $(find src -type f -name *.ts)",
    "prettier": "prettier-check --config .prettierrc $(find src -type f -name *.ts)",
    "prettier:fix": "prettier --config .prettierrc --write $(find src -type f -name *.ts)"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.22",
    "@types/stats": "^0.16.30",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.14.3",
    "prettier-check": "^2.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "dotenv-webpack": "^1.5.7",
    "jquery": "^3.3.1",
    "popmotion": "^8.5.3",
    "sillyname": "^0.1.0",
    "stats.js": "^0.17.0",
    "three": "^0.137.0",
    "unistore": "^3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": "/__tests__/.*\\.test\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  }
}
